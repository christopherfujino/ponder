<!DOCTYPE html>
<html>
  <head>
    <title>Ponder</title>
    <%= csrf_meta_tags %>

    <link rel="stylesheet" href="https://code.jquery.com/jquery-3.2.1.min.js" type="text/css" />
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render partial: 'header' %>
    <div class="container">
      <% flash.each do |key, value| %>
      <p>
        <%= value %>
      </p>
      <hr />
      <% end %>
      <%= yield %>
    </div>


  </body>
</html>
